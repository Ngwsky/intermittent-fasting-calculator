{
  "name": "intermittent-fasting-calculator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 4174",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/smui-dark.css -i src/theme/dark",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@tsconfig/svelte": "^2.0.1",
    "gh-pages": "^4.0.0",
    "smui-theme": "^6.0.0-beta.16",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.7",
    "svelte-preprocess": "^4.9.8",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vite-plugin-pwa": "^0.12.0"
  },
  "dependencies": {
    "@mdi/js": "^6.7.96",
    "@rollup/plugin-json": "^4.1.0",
    "@smui/card": "^6.0.0-beta.16",
    "@smui/checkbox": "^6.0.0-beta.16",
    "@smui/common": "^6.0.0-beta.16",
    "@smui/data-table": "^6.0.0-beta.16",
    "@smui/icon-button": "^6.0.0-beta.16",
    "@smui/layout-grid": "^6.0.0-beta.16",
    "@smui/textfield": "^6.0.0-beta.16",
    "@smui/top-app-bar": "^6.0.0-beta.16",
    "svelte-i18n": "^3.4.0"
  }
}
